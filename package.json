{
    "name": "sync",
    "publisher": "yokowasis",
    "displayName": "sync",
    "description": "Add Syncronization to Code-server",
    "icon": "icon2.jpeg",
    "repository": {
        "type": "git",
        "url": "https://github.com/yokowasis/sync"
    },
    "version": "0.0.6",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./extension.js",
    "contributes": {
        "configuration": {
            "title": "Code GPT",
            "properties": {
                "sync.GistsID": {
                    "title": "Gists ID",
                    "type": "string",
                    "description": "Gists ID to sync with"
                },
                "sync.GithubToken": {
                    "title": "Github User Token",
                    "type": "string",
                    "description": "Github User Token to sync with"
                }
            }
        },
        "commands": [
            {
                "command": "sync.downloadSettings",
                "title": "code-server Sync : Download Settings"
            },
            {
                "command": "sync.uploadSettings",
                "title": "code-server Sync : Upload Settings"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "pnpm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {},
    "dependencies": {
        "shelljs": "^0.8.5"
    },
    "license": "MIT"
}